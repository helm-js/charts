{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es2016"],
    "declaration": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  },
  "include": ["repositories"]
}
